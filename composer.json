{
    "name": "phrity/config",
    "type": "library",
    "description": "Configuration interface, class, and factories",
    "homepage": "https://phrity.sirn.se/net-uri",
    "keywords": ["config", "configuration", "json", "yaml", "neon", "env"],
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ¶ren Jensen",
            "email": "sirn@sirn.se",
            "homepage": "https://phrity.sirn.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phrity\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phrity\\Config\\Test\\": "tests/src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "phrity/util-accessor": "^1.3",
        "phrity/util-transformer": "^1.2.1",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "nette/neon": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0",
        "robiningelbrecht/phpunit-coverage-tools": "^1.9",
        "squizlabs/php_codesniffer": "^3.5 || ^4.0",
        "symfony/dotenv": "^6.0 || ^7.0 || ^8.0",
        "symfony/yaml": "^6.0 || ^7.0 || ^8.0"
    },
    "suggest": {
        "nette/neon": "Required for loading NEON configuration",
        "symfony/dotenv": "Required for loading .env files",
        "symfony/yaml": "Required for loading YAML configuration"
    }
}
