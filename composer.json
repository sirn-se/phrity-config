{
    "name": "phrity/config",
    "type": "library",
    "description": "Configuration interface, class, and factories",
    "homepage": "https://phrity.sirn.se/net-uri",
    "keywords": ["config", "configuration", "json", "yaml", "env"],
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ¶ren Jensen",
            "email": "sirn@sirn.se",
            "homepage": "https://phrity.sirn.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phrity\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phrity\\Config\\Test\\": "tests/src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "phrity/util-accessor": "^1.0",
        "psr/container": "^1.0 | ^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.0 | ^11.0",
        "php-coveralls/php-coveralls": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/dotenv": "^6.0 | ^7.0",
        "symfony/yaml": "^6.0 | ^7.0"
    },
    "suggest": {
        "symfony/dotenv": "Required for loading .env files",
        "symfony/yaml": "Required for loading YAML configuration"
    }
}
